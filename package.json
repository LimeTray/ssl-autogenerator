  "bugs": {
    "url": "https://github.com/Shubhamnegi/ssl-autogenerator/issues"
  },
  "homepage": "https://github.com/Shubhamnegi/ssl-autogenerator#readme",
  "dependencies": {
    "ajv": "^8.6.0",
    "axios": "^0.21.1",
    "bunyan": "^1.8.15",
    "chai": "^4.3.4",
    "dotenv": "^10.0.0",
    "mocha": "^9.0.0",
    "mysql2": "^2.2.5",
    "sequelize": "^6.6.2",
    "typescript": "^4.3.2"
  },
  "devDependencies": {
    "@types/bunyan": "^1.8.6",
    "@types/chai": "^4.2.18",
    "@types/mocha": "^8.2.2"
  }
